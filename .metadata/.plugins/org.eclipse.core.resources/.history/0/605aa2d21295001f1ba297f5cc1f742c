package kr.or.ddit;

public class Main06 {
	
	static void changeVal(int p) {
		p = 3;
	}
	static void changeRdf(int[] p) {
		p[0] =4;
	}
	static void changeStr(String p) {
		p = "chunjae";
	}
	public static void main(String[] args) {
		int a =1;
		int [] b = {2};
		String c = "babo";
		
		System.out.println("a : "+a);
		System.out.println("b[] : "+b[0]);
		changeVal(a);
		changeRdf(b);
		System.out.println("a: "+a);
		System.out.println("b[]: "+b[0]);
		System.out.println("c : "+ c);
	}
}
