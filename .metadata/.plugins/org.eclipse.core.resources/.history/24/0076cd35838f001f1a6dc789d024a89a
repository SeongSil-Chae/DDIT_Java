class Student {
	// 이름 국 영 수 총점 평균 등수 를 필드로 만들것.
	String name;
	int kor;
	int eng;
	int Math;
	int sum;
	double avg;
	int grade;
	@Override
	public String toString() {
		return "Student [name=" + name + ", kor=" + kor + ", eng=" + eng + ", Math=" + Math + ", sum=" + sum + ", avg="
				+ avg + ", grade=" + grade + "]";
	}
	
	// toString 만들기
	
}
