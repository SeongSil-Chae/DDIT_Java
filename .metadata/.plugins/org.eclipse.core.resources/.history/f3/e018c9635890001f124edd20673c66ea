package study06.sec03;

public class Homework11 {
	
}
class Cylinder{
	// 원, 높이, 부피 원의 넓이 * 높이 (생성자 호출시 부피 자동으로 계산)
	double pi = Math.PI; 
	int r;
	int h;
	
	double area;
	
	public Cylinder(int r,double pi,  int h) {
		this.r = r;
		this.h = h;
		this.pi = pi;
		this.area = r*r*pi*h;
	}
}

class Circle{
	private double pi;
	private int r;

	// 원 중심 좌표, 반지름 길이, 넓이:pi*r*r , 원의 둘레: pi*2*r (생성자 호출시 넓이, 둘레 계산되게 할 것)
	
	public Circle(int r, double pi) {
		this.r = r;
		this.pi = pi;
		
	}
}

class Point{
	 // x, y
	
	int x1;
	int y1;
	public Point(int r, int h) {
		this.x1 = r;
		this.y1 = h;
	}
	
}