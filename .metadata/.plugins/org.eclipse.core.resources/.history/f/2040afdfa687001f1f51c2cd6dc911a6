		//          결과 : 이겼다.
		System.out.println("가위(1) 바위(2) 보(3)");
		int user = sc.nextInt();
		System.out.println("가위(1) 바위(2) 보(3)");
		int com = sc.nextInt();
		
		if(user<com) {
//		(int)(Math.random()*3)+1;
	}
}
}