package study02.sec01;

public class VarExample03 {
 public static void main(String[] args) {
	 int x= 10;
	 System.out.println("x = " +x);

	 // int y = 10 선언 및 초기화 y값 출력
	 int y = 11;
	 System.out.println("y = " +y);
	 
	 // y 12저장 후 출력
	 y = 12;
	 System.out.println(y);
	 
	 // int z 에 x+y 값 넣기
	 int z = x + y;
	 System.out.println(z);

}
}
