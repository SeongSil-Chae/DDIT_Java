package study02.sec02;

public class CharExample {
	public static void main(String[] args) {
		// char : 부호없는 2byte 정수(0~65535)
		
		char c1 = 65;
		System.out.println("c1=" +c1); // 출력값이 c1=A 라고 뜸.
		
		char c2 ='a';
		System.out.println("c2="+c2); // 출력값이 c2=a 라고 뜸.
		
		char c3 = (char)(c1+1);
	}
}
