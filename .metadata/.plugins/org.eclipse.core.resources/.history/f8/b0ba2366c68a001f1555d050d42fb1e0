package study04.sec06;

import java.util.Scanner;

public class WileExample {
	Scanner sc = new Scanner(System.in);

	public static void main(String[] args) {
		WileExample obj = new WileExample();
		obj.process();
	}

	public void process() {
		method1();
	}
	
	public void method1() {
		
		// while(조건식){
		//    실행문
		// } 
		while(true) {
			System.out.println("실행문");
		}
	}
}
