	public void method1() {
		int[] arr= {80,91,77,65,-10,-1000};
		// 배열값 두 개의 곱이 최대가 되는값을 출력하시오.
		
		for (int i = 0; i < 10000; i++) {
			int ran = (int)(Math.random()*arr.length);
			int temp = arr[0];
			arr[0] = arr[ran];
			arr[ran] = temp;
		}
		for(int num : arr) System.out.print(num+"\t");
		
		
		int[] num2 = new int[2];
		
		System.arraycopy(num, 0, num2, 0, 2);
		
		int max = num[0]*num[1];
		
		
		
		
	}
