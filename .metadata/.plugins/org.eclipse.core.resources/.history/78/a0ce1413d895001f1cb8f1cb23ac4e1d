package study06.sec06;

public class ttttt {

	public static void main(String[] args) {
		 Person p1 = new Person()
		 p1.greet
	}
}

class Person {
	int age;
	String name;

	public Person(String name) {
		this.age = 0;
		this.name = "Unknown";
	}

	public void greet() {
		System.out.println("hello, my name is " + name);
	}

}
