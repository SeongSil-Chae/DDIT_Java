package kr.or.ddit;

public class Main06 {

	static void changeVal(int p) {
		p = 3;
	}

	static void changeRdf(int[] p) {
		p[0] = 4;
	}

	static void changeStr(String p) {
		p = "chunjae";
	}

	public static void main(String[] args) {
		int a = 0;
		String b = "";
		int[] c = {1, 2, 3, 4, 5};
		
		changeRdf(c.clone());
		a += method1(5);
		
	}

	public int method1(int num) {
		int result = 0;
		for (int i = 0; i < num; i++) {
			result += i;
		}
		
		return result;
	}
}
