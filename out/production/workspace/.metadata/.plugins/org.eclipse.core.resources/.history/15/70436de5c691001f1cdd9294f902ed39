package study11;

public class StringExample {
	public static void main(String[] args) {
		StringExample se = new StringExample();
		se.method1();
	}
	
	public void method1() {
		String test = "abc 입니다.";
		
		// db = like
		if(test.equals("abc 입니다.")) {
			System.out.println("같은 문자열");
		}
		
		if(test.contains("abc")) {
			System.out.println("ab가 포함됨");
		}
	}
}
