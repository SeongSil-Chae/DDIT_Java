package study06.sec06;

public class ttttt {

	public static void main(String[] args) {
		 Person2 p1 = new Person2("asd",1);
		 p1.age =0;
		 p1.name = "1";
		 p1.greet();
	}
}

class Person2 {
	int age;
	String name;

	public Person2(String name, int age) {
		this.age = 0;
		this.name = "Unknown";
		
	}

	public void greet() {
		System.out.println("hello, my name is " + name);
	}

}
